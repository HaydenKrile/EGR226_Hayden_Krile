#include "msp.h"


/**
 * main.c
 */
//uint8_t DebounceSwitch1(void);

void main(void)
{
    P1->SEL1 &= ~2; // configures P1.1 as I/O
    P1->SEL0 &= ~2;
    P1->DIR &= ~2; // P1.1 is set as an input
    P1->REN |= ~2; // P1.1 pull resistor enabled
    P1->OUT |= ~2; // pull resistor is selected by P1->OUT

    P2->SEL1 &= ~7;
    P2->SEL0 &= ~7;
    P2->DIR |= 7;

    while(1){

        if(DebounceSwitch1()){
            P2->OUT |= 1;
        }



    }
/*
        P2->OUT |= 1;
        __delay_cycles(3000000);
        P2->OUT &= ~1;
        P2->OUT |= 2;
        __delay_cycles(3000000);
        P2->OUT &= ~2;
        P2->OUT |= 4;
        __delay_cycles(3000000);
        P2->OUT &= ~4;
    }
*/
}
